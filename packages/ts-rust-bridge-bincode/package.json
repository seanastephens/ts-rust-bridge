{
  "name": "ts-rust-bridge-bincode",
  "version": "0.2.0",
  "description": "A collection of helper functions to serialize data structures in typescript to rust bincode format",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/twop/ts-rust-bridge.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "exclude": [
            "__tests__/**/*.*",
            "examples/**/*.*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  },
  "scripts": {
    "build": "pack build"
  },
  "dependencies": {},
  "devDependencies": {
    "@pika/pack": "^0.3.4",
    "@pika/plugin-build-node": "^0.3.14",
    "@pika/plugin-build-web": "^0.3.14",
    "@pika/plugin-ts-standard-pkg": "^0.3.14",
    "typescript": "3.3.3"
  }
}
