{
  "name": "ts-binary",
  "version": "0.5.0",
  "description": "A collection of helper functions to serialize data structures in typescript to rust bincode format",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/twop/ts-rust-bridge.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "exclude": [
            "__tests__/**/*.*",
            "examples/**/*.*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  },
  "scripts": {
    "build": "pack build",
    "test": "jest"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@pika/pack": "^0.3.4",
    "@pika/plugin-build-node": "^0.3.14",
    "@pika/plugin-build-web": "^0.3.14",
    "@pika/plugin-ts-standard-pkg": "^0.3.14",
    "@types/jest": "^24.0.11",
    "jest": "^24.7.1",
    "ts-jest": "^24.0.2",
    "typescript": "3.3.3"
  }
}
